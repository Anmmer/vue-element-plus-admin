{
  "name": "vue-elementplus-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run mock\" \"npm run serve:dev\"",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "serve:dev": "cross-env NODE_ENV=development dotenv -e .env.dev.serve vue-cli-service serve",
    "build:dev": "cross-env NODE_ENV=production  dotenv -e .env.dev.build vue-cli-service build",
    "serve:prod": "cross-env NODE_ENV=development dotenv -e .env.prod.serve vue-cli-service serve",
    "build:prod": "cross-env NODE_ENV=production  dotenv -e .env.prod.build vue-cli-service build",
    "mock": "cd mock && nodemon generator.js --exec babel-node --presets es2015,stage-2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "clipboard": "^2.0.8",
    "core-js": "^3.8.3",
    "dayjs": "^1.10.4",
    "echarts": "^5.1.1",
    "element-plus": "^1.0.2-beta.40",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsencrypt": "^3.2.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "os": "^0.1.1",
    "qs": "^6.10.1",
    "vue": "^3.0.4",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0-alpha.8",
    "@vue/cli-plugin-eslint": "~5.0.0-alpha.8",
    "@vue/cli-plugin-router": "~5.0.0-alpha.8",
    "@vue/cli-plugin-vuex": "~5.0.0-alpha.8",
    "@vue/cli-service": "~5.0.0-alpha.8",
    "@vue/compiler-sfc": "^3.0.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^6.0.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.2.0",
    "nodemon": "^2.0.7",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "svg-sprite-loader": "^6.0.5"
  }
}
